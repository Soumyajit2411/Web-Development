<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= Content %></p>
<%posts.forEach(function(post){%>
    <h1><%= post.titles%></h1>
    <p><%= post.content. substring(0, 100)+"..." %>
        <a href="/posts/<%= post.titles%>">Read More</a>
    </p>
<%});%>

<%- include('partials/footer'); -%>
